{
    "name": "jmcollin/autoindex",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/autoindex"
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-0": { "AutoIndex": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.16",
        "symplify/easy-coding-standard": "^6.0"
    }
}
